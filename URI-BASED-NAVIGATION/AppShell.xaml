<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="URI_BASED_NAVIGATION.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:URI_BASED_NAVIGATION"
    Shell.FlyoutBehavior="Disabled"
    Title="URI_BASED_NAVIGATION">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent Route="signup" ContentTemplate="{DataTemplate local:MainPage}" />
    <ShellContent Route="profile" ContentTemplate="{DataTemplate local:ProfilePage}" />


</Shell>
